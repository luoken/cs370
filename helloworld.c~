#include <stdio.h>

int main(){
  printf ("Hello world"); 
  int number;

  printf("Enter a number\n");
  scanf("%d", &number);
  printf("you entered %d \n", number);
  printf("%d", number);
  

  char temp;
  printf("\n enter a letter\n");
  scanf(" %c", &temp);
  printf("you entered %c \n", temp);


  for(int x = 0; x< 4; x++){
    printf("hello world\n");
  }










  return 0;

}
